{
  "name": "falconv2",
  "version": "1.0.0",
  "description": "A restful server for Yolo-Messenger",
  "main": "app.js",
  "engineStrict": true,
  "engines": {
    "node": "5.7.0"
  },
  "scripts": {
    "start": "NODE_ENV=development node app.js | bunyan -o short -l warn",
    "start_prod": "node app.js",
    "test": "MONGO_URL='mongodb://localhost:27017/stayyolo-test' ./node_modules/mocha/bin/mocha -c test/ | bunyan -o short -l warn",
    "cover": "MONGO_URL='mongodb://localhost:27017/stayyolo-test' node_modules/istanbul/lib/cli.js cover node_modules/.bin/_mocha -- -c test/ | bunyan -o short -l warn"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/stayyolo/falconv2.git"
  },
  "author": "StayYolo (hello@stayyolo.com)",
  "private": true,
  "homepage": "https://bitbucket.org/stayyolo/falconv2#readme",
  "dependencies": {
    "agenda": "0.8.0",
    "bluebird": "3.3.3",
    "bunyan": "1.7.0",
    "bunyan-loggly": "1.0.0",
    "cheerio": "0.20.0",
    "lodash": "4.5.1",
    "moment": "2.11.2",
    "mongodb": "2.1.7",
    "node-wit": "2.0.0",
    "request-promise": "2.0.1",
    "restify": "4.0.4",
    "restify-errors": "4.2.0",
    "restify-validator": "0.3.1",
    "xmldom": "0.1.22",
    "xpath": "0.0.21"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "5.2.0",
    "chai-joi": "1.1.0",
    "dockerode": "2.2.9",
    "dotenv": "2.0.0",
    "gulp": "3.9.1",
    "gulp-help": "1.6.1",
    "gulp-util": "3.0.7",
    "istanbul": "0.4.2",
    "joi": "8.0.3",
    "jscs": "2.10.1",
    "mocha": "2.4.5",
    "require-dir": "0.3.0",
    "stream": "0.0.2",
    "supertest": "1.2.0",
    "supertest-as-promised": "3.0.0",
    "tar-fs": "1.10.0",
    "tutum": "0.3.2",
    "ws": "1.0.1"
  }
}
